REDIS_DB=                     # RedisDBID

# 应用配置
APP_PORT=                     # 监听端口
APP_ENV=production
LOG_LEVEL=info
CLOUD_FILE_DIR=               # 服务器云盘文件存储路径
MAX_FILE_SIZE=                # 单个文件最大大小（单位：GB
NORMAL_USER_MAX_STORAGE=      # 非VIP用户储存限额
LIMITED_SPEED=                # 非VIP用户下载速度限额 为0则不限速